#: /usr/lib/python3.10/argparse.py:296 /usr/lib/python3.11/argparse.py:299
#: /usr/lib/python3.12/argparse.py:299 /usr/lib/python3.9/argparse.py:308
msgid "usage: "
msgstr "Aufruf: "

#: /usr/lib/python3.10/argparse.py:868 /usr/lib/python3.11/argparse.py:882
#: /usr/lib/python3.12/argparse.py:883 /usr/lib/python3.9/argparse.py:866
msgid ".__call__() not defined"
msgstr ".__call__() ist undefiniert"

#: /usr/lib/python3.10/argparse.py:1211 /usr/lib/python3.11/argparse.py:1231
#: /usr/lib/python3.12/argparse.py:1253 /usr/lib/python3.9/argparse.py:1209
#, python-format
msgid "unknown parser %(parser_name)r (choices: %(choices)s)"
msgstr "unbekannter Parser %(parser_name)r (Auswahl aus %(choices)s)"

#: /usr/lib/python3.10/argparse.py:1271 /usr/lib/python3.11/argparse.py:1291
#: /usr/lib/python3.12/argparse.py:1313 /usr/lib/python3.9/argparse.py:1269
#, python-format
msgid "argument \"-\" with mode %r"
msgstr "Parameter \"-\" mit Zugriffsart %r"

#: /usr/lib/python3.10/argparse.py:1280 /usr/lib/python3.11/argparse.py:1300
#: /usr/lib/python3.12/argparse.py:1322 /usr/lib/python3.9/argparse.py:1278
#, python-format
msgid "can't open '%(filename)s': %(error)s"
msgstr "kann '%(filename)s' nicht öffnen: %(error)s"

#: /usr/lib/python3.10/argparse.py:1489 /usr/lib/python3.11/argparse.py:1509
#: /usr/lib/python3.12/argparse.py:1531 /usr/lib/python3.9/argparse.py:1487
#, python-format
msgid "cannot merge actions - two groups are named %r"
msgstr ""
"kann Aktionen nicht zusammenführen - zwei Gruppen haben den gleichen Namen %r"

#: /usr/lib/python3.10/argparse.py:1527 /usr/lib/python3.11/argparse.py:1547
#: /usr/lib/python3.12/argparse.py:1569 /usr/lib/python3.9/argparse.py:1525
msgid "'required' is an invalid argument for positionals"
msgstr "'required' darf nicht mit Positionsparametern verwendet werden"

#: /usr/lib/python3.10/argparse.py:1549 /usr/lib/python3.11/argparse.py:1569
#: /usr/lib/python3.12/argparse.py:1591 /usr/lib/python3.9/argparse.py:1547
#, python-format
msgid ""
"invalid option string %(option)r: must start with a character "
"%(prefix_chars)r"
msgstr ""
"Option %(option)r ungültig: muss mit einem Zeichen %(prefix_chars)r beginnen"

#: /usr/lib/python3.10/argparse.py:1567 /usr/lib/python3.11/argparse.py:1587
#: /usr/lib/python3.12/argparse.py:1609 /usr/lib/python3.9/argparse.py:1565
#, python-format
msgid "dest= is required for options like %r"
msgstr "dest= wird bei Optionen wie %r benötigt"

#: /usr/lib/python3.10/argparse.py:1584 /usr/lib/python3.11/argparse.py:1604
#: /usr/lib/python3.12/argparse.py:1626 /usr/lib/python3.9/argparse.py:1582
#, python-format
msgid "invalid conflict_resolution value: %r"
msgstr "ungültiger Wert für conflict_resolution: %r"

#: /usr/lib/python3.10/argparse.py:1602 /usr/lib/python3.11/argparse.py:1622
#: /usr/lib/python3.12/argparse.py:1644 /usr/lib/python3.9/argparse.py:1600
#, python-format
msgid "conflicting option string: %s"
msgid_plural "conflicting option strings: %s"
msgstr[0] "Option steht im Konflikt: %s"
msgstr[1] "Optionen stehen im Konflikt: %s"

#: /usr/lib/python3.10/argparse.py:1668 /usr/lib/python3.11/argparse.py:1696
#: /usr/lib/python3.12/argparse.py:1718 /usr/lib/python3.9/argparse.py:1666
msgid "mutually exclusive arguments must be optional"
msgstr "sich gegenseitig ausschließende Parameter müssen optional sein"

#: /usr/lib/python3.10/argparse.py:1736 /usr/lib/python3.11/argparse.py:1772
#: /usr/lib/python3.12/argparse.py:1794 /usr/lib/python3.9/argparse.py:1733
msgid "positional arguments"
msgstr "Positionsparameter"

#: /usr/lib/python3.10/argparse.py:1737 /usr/lib/python3.11/argparse.py:1773
#: /usr/lib/python3.12/argparse.py:1795
msgid "options"
msgstr "Optionen"

#: /usr/lib/python3.10/argparse.py:1752 /usr/lib/python3.11/argparse.py:1788
#: /usr/lib/python3.12/argparse.py:1810 /usr/lib/python3.9/argparse.py:1749
msgid "show this help message and exit"
msgstr "diese Meldung anzeigen und beenden"

#: /usr/lib/python3.10/argparse.py:1783 /usr/lib/python3.11/argparse.py:1819
#: /usr/lib/python3.12/argparse.py:1841 /usr/lib/python3.9/argparse.py:1780
msgid "cannot have multiple subparser arguments"
msgstr "mehrfache Subparser werden nicht unterstützt"

#: /usr/lib/python3.10/argparse.py:1835 /usr/lib/python3.10/argparse.py:2350
#: /usr/lib/python3.11/argparse.py:1871 /usr/lib/python3.11/argparse.py:2384
#: /usr/lib/python3.12/argparse.py:1893 /usr/lib/python3.12/argparse.py:2408
#: /usr/lib/python3.9/argparse.py:1832 /usr/lib/python3.9/argparse.py:2343
#, python-format
msgid "unrecognized arguments: %s"
msgstr "Parameter nicht erkannt: %s"

#: /usr/lib/python3.10/argparse.py:1936 /usr/lib/python3.11/argparse.py:1971
#: /usr/lib/python3.12/argparse.py:1993 /usr/lib/python3.9/argparse.py:1933
#, python-format
msgid "not allowed with argument %s"
msgstr "in Verbindung mit Parameter %s nicht erlaubt"

#: /usr/lib/python3.10/argparse.py:1986 /usr/lib/python3.10/argparse.py:2000
#: /usr/lib/python3.11/argparse.py:2021 /usr/lib/python3.11/argparse.py:2035
#: /usr/lib/python3.12/argparse.py:2043 /usr/lib/python3.12/argparse.py:2057
#: /usr/lib/python3.9/argparse.py:1979 /usr/lib/python3.9/argparse.py:1993
#, python-format
msgid "ignored explicit argument %r"
msgstr "expliziten Parameter %r ignoriert"

#: /usr/lib/python3.10/argparse.py:2107 /usr/lib/python3.11/argparse.py:2142
#: /usr/lib/python3.12/argparse.py:2164 /usr/lib/python3.9/argparse.py:2100
#, python-format
msgid "the following arguments are required: %s"
msgstr "die folgenden Parameter werden benötigt: %s"

#: /usr/lib/python3.10/argparse.py:2122 /usr/lib/python3.11/argparse.py:2157
#: /usr/lib/python3.12/argparse.py:2179 /usr/lib/python3.9/argparse.py:2115
#, python-format
msgid "one of the arguments %s is required"
msgstr "einer der Parameter %s wird benötigt"

#: /usr/lib/python3.10/argparse.py:2165 /usr/lib/python3.11/argparse.py:2199
#: /usr/lib/python3.12/argparse.py:2223 /usr/lib/python3.9/argparse.py:2158
msgid "expected one argument"
msgstr "genau ein Parameter wird erwartet"

#: /usr/lib/python3.10/argparse.py:2166 /usr/lib/python3.11/argparse.py:2200
#: /usr/lib/python3.12/argparse.py:2224 /usr/lib/python3.9/argparse.py:2159
msgid "expected at most one argument"
msgstr "höchstens ein Parameter wird erwartet"

#: /usr/lib/python3.10/argparse.py:2167 /usr/lib/python3.11/argparse.py:2201
#: /usr/lib/python3.12/argparse.py:2225 /usr/lib/python3.9/argparse.py:2160
msgid "expected at least one argument"
msgstr "mindestens ein Parameter wird erwartet"

#: /usr/lib/python3.10/argparse.py:2171 /usr/lib/python3.11/argparse.py:2205
#: /usr/lib/python3.12/argparse.py:2229 /usr/lib/python3.9/argparse.py:2164
#, python-format
msgid "expected %s argument"
msgid_plural "expected %s arguments"
msgstr[0] "%s Parameter wird erwartet"
msgstr[1] "%s Parameter werden erwartet"

#: /usr/lib/python3.10/argparse.py:2229 /usr/lib/python3.11/argparse.py:2263
#: /usr/lib/python3.12/argparse.py:2287 /usr/lib/python3.9/argparse.py:2222
#, python-format
msgid "ambiguous option: %(option)s could match %(matches)s"
msgstr "Option nicht eindeutig: %(option)s passt auf %(matches)s"

#: /usr/lib/python3.10/argparse.py:2293 /usr/lib/python3.11/argparse.py:2327
#: /usr/lib/python3.12/argparse.py:2351 /usr/lib/python3.9/argparse.py:2286
#, python-format
msgid "unexpected option string: %s"
msgstr "unerwartete Option: %s"

#: /usr/lib/python3.10/argparse.py:2490 /usr/lib/python3.11/argparse.py:2524
#: /usr/lib/python3.12/argparse.py:2550 /usr/lib/python3.9/argparse.py:2483
#, python-format
msgid "%r is not callable"
msgstr "%r ist nicht aufrufbar"

#: /usr/lib/python3.10/argparse.py:2507 /usr/lib/python3.11/argparse.py:2541
#: /usr/lib/python3.12/argparse.py:2566 /usr/lib/python3.9/argparse.py:2500
#, python-format
msgid "invalid %(type)s value: %(value)r"
msgstr "ungültiger Wert für %(type)s: %(value)r"

#: /usr/lib/python3.10/argparse.py:2518 /usr/lib/python3.11/argparse.py:2552
#: /usr/lib/python3.12/argparse.py:2577 /usr/lib/python3.9/argparse.py:2511
#, python-format
msgid "invalid choice: %(value)r (choose from %(choices)s)"
msgstr "ungültiger Wert: %(value)r (Auswahl aus %(choices)s)"

#: /usr/lib/python3.10/argparse.py:2594 /usr/lib/python3.11/argparse.py:2630
#: /usr/lib/python3.12/argparse.py:2655 /usr/lib/python3.9/argparse.py:2587
#, python-format
msgid "%(prog)s: error: %(message)s\n"
msgstr "%(prog)s: Fehler: %(message)s\n"

#: /usr/lib/python3.11/argparse.py:776 /usr/lib/python3.12/argparse.py:777
#, python-format
msgid "argument %(argument_name)s: %(message)s"
msgstr "Parameter %(argument_name)s: %(message)s"

#: /usr/lib/python3.11/argparse.py:1192 /usr/lib/python3.12/argparse.py:1214
#, python-format
msgid "conflicting subparser: %s"
msgstr "Subparser im Konflikt: %s"

#: /usr/lib/python3.11/argparse.py:1196 /usr/lib/python3.12/argparse.py:1218
#, python-format
msgid "conflicting subparser alias: %s"
msgstr "Subparser-Alias im Konflikt: %s"

#: /usr/lib/python3.9/argparse.py:1734
msgid "optional arguments"
msgstr "Optionen"
